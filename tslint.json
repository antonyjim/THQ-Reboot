{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "no-console": false,
    "indent": [true, "spaces", 2],
    "semicolon": false,
    "triple-equals": true,
    "curly": [true, "ignore-same-line"]
  }
}